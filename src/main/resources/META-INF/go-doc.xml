<idea-plugin>
    <extensions defaultExtensionNs="cn.yiiguxing.plugin.translate">
        <docElementProvider
                language="go"
                implementationClass="cn.yiiguxing.plugin.translate.provider.GoDocumentationElementProvider"/>
    </extensions>

    <extensions defaultExtensionNs="com.intellij">
        <lang.documentationProvider
                language="go" order="first"
                implementationClass="cn.yiiguxing.plugin.translate.provider.TranslatingDocumentationProvider"/>
    </extensions>
</idea-plugin>